#include <iostream>
using namespace std;
int main()
{
    int t;
    cin >> t;
    while (t--)
    {
        float s,a, b, c,  p, x, y;
        cin >>  s;
        cin >>  a;
        cin >>  b;
        cin >>  c;

        x = (c * s) / 100;
        y = x + s;

        if (y >= a && y <= b)
        {
            printf("yes");
        }
        else
        {
            printf("no");
        }
    }

    return 0;
}
